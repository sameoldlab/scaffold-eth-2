{
  "name": "se-2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
		
		"account": "pnpm -F @se-2/foundry account",
		"chain": "pnpm -F @se-2/foundry chain",
		"compile": "pnpm -F @se-2/foundry compile",
		"deploy": "pnpm -F @se-2/foundry deploy",
		"deploy:verify": "pnpm -F @se-2/foundry deploy:verify",
		"fork": "pnpm -F @se-2/foundry fork",
		"foundry:lint": "pnpm -F @se-2/foundry lint",
		"foundry:test": "pnpm -F @se-2/foundry test",
		"generate": "pnpm -F @se-2/foundry generate",
		"postinstall": "husky install",
		"next:check-types": "pnpm -F @se-2/nextjs check-types",
		"next:format": "pnpm -F @se-2/nextjs format",
		"next:lint": "pnpm -F @se-2/nextjs lint",
		"precommit": "lint-staged",
		"start": "pnpm -F @se-2/nextjs dev",
		"test": "pnpm foundry:test",
		"vercel": "pnpm -F @se-2/nextjs vercel",
		"vercel:yolo": "pnpm -F @se-2/nextjs vercel:yolo",
		"verify": "pnpm -F @se-2/foundry verify"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3"
  }
}
